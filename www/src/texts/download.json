{
    "opts": {
        "prebuild": {
            "typ-dylib": {
                "display_name": "Dymatic library",
                "title": "Dymatic library prebuild"
            },
            "typ-lib": {
                "display_name": "Static library",
                "title": "Static library prebuild"
            },
            "typ-wasm": {
                "display_name": "WASM",
                "title": "WASM prebuild"
            },
            "typ-cmsis-pack": {
                "display_name": "CMSIS Pack",
                "title": "CMSIS pack"
            },
            "typ-src": {
                "display_name": "Sources",
                "title": "Source code"
            }
        },
        "OS": {
            "os-win": {
                "display_name": "Windows",
                "requires": [
                    "typ-lib",
                    "typ-dylib"
                ]
            },
            "os-unix": {
                "display_name": "Linux",
                "requires": [
                    "typ-lib",
                    "typ-dylib"
                ]
            },
            "os-none": {
                "display_name": "None",
                "requires": [
                    "typ-wasm",
                    "typ-src",
                    "typ-lib",
                    "typ-cmsis-pack"
                ]
            }
        },
        "ISA/CPU": {
            "cpu-cortex-m0": {
                "display_name": "ARM Cortex-M0",
                "requires": [
                    "typ-lib&os-none"
                ]
            },
            "cpu-cortex-m0+": {
                "display_name": "ARM Cortex-M0+",
                "requires": [
                    "typ-lib&os-none"
                ]
            },
            "cpu-cortex-m3": {
                "display_name": "ARM Cortex-M3",
                "requires": [
                    "typ-lib&os-none"
                ]
            },
            "cpu-cortex-m4": {
                "display_name": "ARM Cortex-M4",
                "requires": [
                    "typ-lib&os-none"
                ]
            },
            "cpu-cortex-m7": {
                "display_name": "ARM Cortex-M7",
                "requires": [
                    "typ-lib&os-none"
                ]
            },
            "cpu-cortex-m23": {
                "display_name": "ARM Cortex-M23",
                "requires": [
                    "typ-lib&os-none"
                ]
            },
            "cpu-cortex-m33": {
                "display_name": "ARM Cortex-M33",
                "requires": [
                    "typ-lib&os-none"
                ]
            },
            "cpu-cortex-m55": {
                "display_name": "ARM Cortex-M55",
                "requires": [
                    "typ-lib&os-none"
                ]
            },
            "cpu-amd64": {
                "display_name": "AMD64",
                "requires": [
                    "typ-dylib",
                    "typ-lib",
                    "os-win",
                    "os-unix"
                ]
            },
            "cpu-x86": {
                "display_name": "x86",
                "requires": [
                    "typ-dylib",
                    "typ-lib",
                    "os-win",
                    "os-unix"
                ]
            },
            "cpu-wasm": {
                "display_name": "WASM",
                "requires": [
                    "typ-wasm&os-none"
                ]
            },
            "cpu-any": {
                "display_name": "Any",
                "requires": [
                    "typ-src",
                    "typ-cmsis-pack"
                ]
            }
        }
    },
    "download": [
        {
            "cond": "typ-src",
            "type": "hyper",
            "addr": "https://github.com/MtFmT-Lib/mtfmt/archive/refs/heads/master.zip"
        },
        {
            "cond": "typ-wasm&os-none&cpu-wasm",
            "type": "artifact",
            "addr": "build-wasm.yml",
            "build": {
                "utf-8": true,
                "malloc": true,
                "hardware_div": false,
                "std_io": false,
                "align": 4,
                "fp16": false,
                "fp32": false,
                "fp64": false
            }
        },
        {
            "cond": "typ-cmsis-pack&os-none",
            "type": "artifact",
            "addr": "cmsis-pack.yml"
        },
        {
            "cond": "typ-lib&os-none&cpu-cortex-m3",
            "type": "artifact",
            "addr": "build-staticlib.yml",
            "file": "gcc-lib-m3",
            "build": {
                "malloc": false,
                "hardware_div": true,
                "std_io": false,
                "align": 4,
                "fp16": false,
                "fp32": false,
                "fp64": false
            }
        }
    ],
    "default": {
        "prebuild": "typ-dylib",
        "OS": "os-win",
        "ISA/CPU": "cpu-x86"
    },
    "configure": {
        "utf-8": "_MSTR_USE_UTF_8",
        "malloc": "_MSTR_USE_MALLOC",
        "hardware_div": "_MSTR_USE_HARDWARE_DIV",
        "std_io": "_MSTR_USE_STD_IO",
        "align": "_MSTR_RUNTIME_HEAP_ALIGN",
        "fp16": "_MSTR_USE_FP_FLOAT16",
        "fp32": "_MSTR_USE_FP_FLOAT32",
        "fp64": "_MSTR_USE_FP_FLOAT64"
    }
}